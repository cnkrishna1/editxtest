<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">

	<xs:include schemaLocation="EDItX_CodeLists.xsd" />

	<xs:element name="SalesReport">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Header" />
				<xs:element ref="ItemDetail" maxOccurs="unbounded" />
				<xs:element ref="Summary" />
			</xs:sequence>
			<xs:attribute name="version" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="1.1" />
						<xs:enumeration value="1.2" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>

	<xs:element name="Header">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SalesReportNumber" />
				<xs:element ref="IssueDateTime" />
				<xs:element ref="ReferenceCoded" minOccurs="0" maxOccurs="unbounded" /> <!-- 1.2: added -->
				<xs:element ref="PurposeCode" />
				<xs:element ref="SalesReportTypeCode" minOccurs="0" />
				<xs:element ref="ReportPriceType" minOccurs="0" />
				<xs:choice minOccurs="0">
					<xs:element ref="CurrencyCode" />                       <!-- 1.2: deprecated here -->
					<xs:element ref="CountryCode" minOccurs="0" />          <!-- 1.2: deprecated here -->
				</xs:choice>
				<xs:choice minOccurs="0">
					<xs:sequence>
						<xs:element ref="FromDate" />                       <!-- 1.2: deprecated -->
						<xs:element ref="ToDate" />                         <!-- 1.2: deprecated -->
					</xs:sequence>
					<xs:element ref="DateCoded" maxOccurs="unbounded" />
				</xs:choice>
				<xs:element ref="ClassOfSaleCode" minOccurs="0" maxOccurs="unbounded" />
				<xs:choice minOccurs="0">
					<xs:element ref="TradingZone" />                        <!-- 1.2: deprecated here -->
					<xs:element ref="Territory" />                          <!-- 1.2: added -->
				</xs:choice>
				<xs:choice>
					<!-- Reseller options -->
					<xs:sequence>
						<xs:element ref="SellerParty" />
						<xs:choice>
							<xs:sequence>
								<xs:choice>
									<xs:element ref="PublisherParty" />
									<xs:element ref="SupplierParty" />
								</xs:choice>
								<xs:element ref="BuyerParty" minOccurs="0" maxOccurs="unbounded" />
								<xs:element ref="ReportingParty" minOccurs="0" />
								<xs:element ref="ReceivingParty" minOccurs="0" />
							</xs:sequence>
							<xs:sequence>
								<xs:element ref="BuyerParty" minOccurs="0" maxOccurs="unbounded" />
								<xs:element ref="ReportingParty" minOccurs="0" />
								<xs:element ref="ReceivingParty" />
							</xs:sequence>
						</xs:choice>
					</xs:sequence>
					<!-- Agency options -->
					<xs:sequence>
						<xs:choice>
							<xs:element ref="PublisherParty" />
							<xs:element ref="SupplierParty" />
						</xs:choice>
						<xs:element ref="ReportingParty" />
						<xs:element ref="ReceivingParty" minOccurs="0" />
						<xs:element ref="SellingAgentParty" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
					<xs:sequence>
						<xs:element ref="ReportingParty" />
						<xs:element ref="ReceivingParty" />
						<xs:element ref="SellingAgentParty" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ItemDetail">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LineNumber" />
				<xs:element ref="ProductIdentifier" maxOccurs="unbounded" />
				<xs:element ref="ItemDescription" minOccurs="0" />
				<xs:element ref="PublisherDetail" minOccurs="0" />
				<xs:element ref="ImprintDetail" minOccurs="0" />
				<xs:element ref="FormatCode" minOccurs="0" maxOccurs="unbounded" />
				<xs:element ref="EpubTechnicalProtection" minOccurs="0" />                   <!-- 1.2: added -->
				<xs:element ref="EpubUsageConstraint" minOccurs="0" maxOccurs="unbounded" /> <!-- 1.2: added -->
				<xs:element ref="EpubLicense" minOccurs="0" />                               <!-- 1.2: added -->
				<xs:element ref="PriceCondition" minOccurs="0" maxOccurs="unbounded" />      <!-- 1.2: added -->
				<xs:element ref="DeviceType" minOccurs="0" />                                <!-- 1.2: deprecated -->
				<xs:choice minOccurs="0">
					<xs:element ref="Quantity" />
					<xs:sequence>
						<xs:element ref="GrossSoldQuantity" />
						<xs:element ref="RefundedQuantity" />
						<xs:element ref="NetSoldQuantity" />
					</xs:sequence>
				</xs:choice>
				<xs:element ref="NonSaleDisposal" minOccurs="0" maxOccurs="unbounded" />
				<xs:element ref="ReferenceCoded" minOccurs="0" maxOccurs="unbounded" />
				<xs:element ref="ShipToParty" minOccurs="0" />
				<xs:element ref="ShipToLocation" minOccurs="0" />
				<xs:element ref="BillToParty" minOccurs="0" />
				<xs:element ref="BillToLocation" minOccurs="0" />
				<xs:choice minOccurs="0">
					<xs:element name="DateCoded"> <!-- DateQualifierCode defined locally to limit enumeration -->
						<xs:complexType>
							<xs:sequence>
								<xs:element ref="Date" />
								<xs:element name="DateQualifierCode">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="TransactionDate" />
											<xs:enumeration value="TransactionDateTime" />
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element ref="Date" />
				</xs:choice>
				<xs:element ref="PricingDetail" maxOccurs="unbounded" />
				<xs:element ref="LineSalesAmounts" minOccurs="0" />
				<xs:choice minOccurs="0">
					<xs:element ref="RetailerParty" />
					<xs:element ref="SellingAgentParty" />
				</xs:choice>
				<xs:choice minOccurs="0">
					<xs:element ref="TradingZone" />                        <!-- 1.2: deprecated -->
					<xs:element ref="Territory" />
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Summary">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="NumberOfLines" />
				<xs:element ref="NumberOfUnits" minOccurs="0" />
				<xs:sequence minOccurs="0">
					<xs:element ref="NumberOfGrossSalesUnits" />
					<xs:element ref="NumberOfRefundedUnits" />
					<xs:element ref="NumberOfNetSalesUnits" />
				</xs:sequence>
				<xs:element ref="NumberOfNonSaleUnits" minOccurs="0" />
				<xs:element ref="TotalSalesAmounts" minOccurs="0" maxOccurs="unbounded" />
				<xs:choice>
					<xs:element ref="TotalDueToPublisher" maxOccurs="unbounded" />
					<xs:element ref="TotalDueToSupplier" maxOccurs="unbounded" />
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="AdditionalPartyIdentifier" type="SRPartyID" />
	<xs:element name="AddressLine" type="xs:string" />
	<xs:element name="TaxableAmount" type="xs:decimal" />
	<xs:element name="Contributor" type="xs:string" />
	<xs:element name="BillToLocation">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LocationIdentifier" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BillToParty" type="SRParty" />
	<xs:element name="BuyerParty" type="SRParty" />
	<xs:element name="ChargeQualifierCode" type="SRChargeQualifierCode" />
	<xs:element name="ChargeSourceCode" type="SRChargeSourceCode" />
	<xs:element name="ChargeTypeCode" type="SRChargeTypeCode" />
	<xs:element name="ChargeTypeDescription" type="xs:string" />
	<xs:element name="ProductClassificationCode" type="xs:string" />
	<xs:element name="ProductClassification">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ProductClassificationType" />
				<xs:element ref="ProductClassificationCode" />
				<xs:element ref="Percent" minOccurs="0" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ProductClassificationType" type="SRClassificationSchemeCode" />
	<xs:element name="ClassOfSaleCode" type="SRClassOfSaleCode" />
	<xs:element name="CommissionDiscountPercentage" type="SRPercent" />
	<xs:element name="CommunicationDetails">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CommunicationTypeCode" />
				<xs:element ref="CommunicationLocator" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CommunicationLocator" type="xs:string" />
	<xs:element name="CommunicationTypeCode" type="CommunicationTypeCode" />
	<xs:element name="ContactPerson">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ContactRoleCode" minOccurs="0"/>
				<xs:element ref="PersonName" />
				<xs:element ref="CommunicationDetails" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactRoleCode">
		<xs:simpleType>
			<xs:union memberTypes="List198 xs:string" />
		</xs:simpleType>
	</xs:element>
	<xs:element name="CountryCode" type="CountryCode" />
	<xs:element name="CurrencyCode" type="CurrencyCode" />
	<xs:element name="CurrencyConversionRate" type="xs:decimal" />
	<xs:element name="Date" type="SRDateOrDateTime" />
	<xs:element name="DateCoded">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Date" />
				<xs:element ref="DateQualifierCode" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DateQualifierCode" type="SRDateQualifierCode" />
	<xs:element name="DeviceType" type="SRDeviceTypeCode" />
	<xs:element name="DiscountsAmount" type="xs:decimal" />
	<xs:element name="DiscountPercentage" type="SRPercent" />
	<xs:element name="DisposalQuantity" type="xs:int" />
	<xs:element name="DisposalType" type="SRNonSaleDisposalTypeCode" />
	<xs:element name="FormatCode">
		<xs:simpleType>
			<xs:union memberTypes="SRFormatCode List150 List175 xs:string" />
		</xs:simpleType>
	</xs:element>
	<xs:element name="FromDate" type="SRDateOrDateTime" />
	<xs:element name="GrossSalesAmount" type="xs:decimal" />
	<xs:element name="GrossSalesAmountBeforeDiscounts" type="xs:decimal" />
	<xs:element name="GrossSoldQuantity" type="xs:int" />
	<xs:element name="IDValue" type="xs:string" />
	<xs:element name="IDTypeName" type="xs:string" />
	<xs:element name="ImprintIdentifier">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ImprintIDType" />
				<xs:element ref="IDTypeName" minOccurs="0" />
				<xs:element ref="IDValue" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ImprintIDType" type="SRImprintIDType" />
	<xs:element name="ImprintDetail">
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:element ref="ImprintIdentifier" maxOccurs="unbounded" />
					<xs:element ref="ImprintName" minOccurs="0" />
				</xs:sequence>
				<xs:element ref="ImprintName" />
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="ImprintName" type="xs:string" />
	<xs:element name="IssueDateTime" type="SRDateOrDateTime" />
	<xs:element name="ItemDescription">
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element ref="TitleDetail" />
					<xs:sequence>
						<xs:element ref="Title" />
						<xs:element ref="Contributor" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:choice>
				<xs:element ref="OtherDetail" minOccurs="0" />
				<xs:element ref="ProductClassification" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LineNumber" type="xs:positiveInteger" />
	<xs:element name="LineSalesAmounts">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="GrossSalesAmountBeforeDiscounts" minOccurs="0" />
				<xs:element ref="DiscountsAmount" minOccurs="0" />
				<xs:element ref="GrossSalesAmount" minOccurs="0" />
				<xs:element ref="RefundedAmount" minOccurs="0" />
				<xs:element ref="NetSalesAmount" minOccurs="0" />
				<xs:element ref="SellerCharge" minOccurs="0" maxOccurs="unbounded" />
				<xs:choice minOccurs="0">
					<xs:element ref="LineAmountDueToPublisher" />
					<xs:element ref="LineAmountDueToSupplier" />
				</xs:choice>
				<xs:element ref="CurrencyCode" minOccurs="0" />
				<xs:choice minOccurs="0">
					<xs:element ref="CountryCode" />
					<xs:element ref="RegionCode" />
					<xs:element ref="Territory" />
				</xs:choice>
				<xs:choice minOccurs="0">
					<xs:element ref="Tax" maxOccurs="unbounded" />
					<xs:element ref="TaxExempt" />
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LocationIdentifier">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LocationIDType" />
				<xs:element ref="IDTypeName" minOccurs="0" />
				<xs:element ref="IDValue" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LocationIDType" type="SRShipToLocationIDAgencyCode" />
	<xs:element name="MonetaryAmount" type="xs:decimal" />
	<xs:element name="NameLine" type="xs:string" />
	<xs:element name="NetSalesAmount" type="xs:decimal" />
	<xs:element name="NetSoldQuantity" type="xs:int" />
	<xs:element name="NonSaleDisposal">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DisposalType" />
				<xs:element ref="DisposalQuantity" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="NumberOfLines" type="xs:int" />
	<xs:element name="NumberOfUnits" type="xs:int" />
	<xs:element name="NumberOfGrossSalesUnits" type="xs:int" />
	<xs:element name="NumberOfRefundedUnits" type="xs:int" />
	<xs:element name="NumberOfNetSalesUnits" type="xs:int" />
	<xs:element name="NumberOfNonSaleUnits" type="xs:int" />
	<xs:element name="OtherDetail" type="xs:string" />
	<xs:element name="PartyIdentifier" type="SRPartyID" />
	<xs:element name="PartyIDType" type="SRPartyIDType" />
	<xs:element name="PartyName" type="SRPartyName" />
	<xs:element name="TaxRatePercent" type="SRPercent" />
	<xs:element name="PersonName" type="xs:string" />
	<xs:element name="PostalAddress">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AddressLine" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Price">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PriceIdentifier" minOccurs="0" maxOccurs="unbounded" />          <!-- 1.2: added -->
				<xs:choice>
					<xs:sequence>
						<xs:element ref="PriceType" />                                            <!-- 1.2: added -->
						<xs:element ref="PriceQualifier" minOccurs="0" />                         <!-- 1.2: added -->
						<xs:element ref="DiscountCoded" minOccurs="0" maxOccurs="unbounded" />    <!-- 1.2: added -->
						<xs:element ref="Discount" minOccurs="0" maxOccurs="unbounded" />         <!-- 1.2: added -->
						<xs:choice>
							<xs:element ref="MonetaryAmount" />                                   <!-- 1.2: deprecated here -->
							<xs:element ref="PriceAmount" />                                      <!-- 1.2: added -->
						</xs:choice>
						<xs:element ref="CurrencyCode" minOccurs="0" />
						<xs:choice minOccurs="0">
							<xs:element ref="CountryCode" />
							<xs:element ref="RegionCode" />                                       <!-- 1.2: added -->
							<xs:element ref="Territory" />                                        <!-- 1.2: added -->
						</xs:choice>
						<xs:choice minOccurs="0">
							<xs:element ref="Tax" maxOccurs="unbounded" />
							<xs:element ref="TaxExempt" />                                        <!-- 1.2: added -->
						</xs:choice>
					</xs:sequence>
					<xs:sequence>
						<xs:element ref="DiscountCoded" minOccurs="0" maxOccurs="unbounded" />    <!-- 1.2: added -->
						<xs:element ref="Discount" minOccurs="0" maxOccurs="unbounded" />         <!-- 1.2: added -->
						<xs:choice>
							<xs:element ref="MonetaryAmount" />                                   <!-- 1.2: deprecated here -->
							<xs:element ref="PriceAmount" />                                      <!-- 1.2: added -->
						</xs:choice>
						<xs:element ref="CurrencyCode" minOccurs="0" />
						<xs:choice minOccurs="0">
							<xs:element ref="CountryCode" />
							<xs:element ref="RegionCode" />                                       <!-- 1.2: added -->
							<xs:element ref="Territory" />                                        <!-- 1.2: added -->
						</xs:choice>
						<xs:element ref="PriceQualifierCode" minOccurs="0" />                     <!-- 1.2: no longer mandatory (use sequence above instead) -->
						<xs:choice minOccurs="0">
							<xs:element ref="Tax" maxOccurs="unbounded" />
							<xs:element ref="TaxExempt" />                                        <!-- 1.2: added -->
						</xs:choice>
					</xs:sequence>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PricingDetail">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ClassOfSaleCode" minOccurs="0" maxOccurs="unbounded" />
				<xs:element ref="Price" />
				<xs:choice minOccurs="0">
					<xs:element ref="DiscountPercentage" />
					<xs:element ref="CommissionDiscountPercentage" />
				</xs:choice>
				<xs:element ref="CurrencyConversionRate" minOccurs="0" />
				<xs:element ref="SellerCharge" minOccurs="0" maxOccurs="unbounded" />
				<xs:choice minOccurs="0">
					<xs:element ref="UnitAmountDueToPublisher" />
					<xs:element ref="UnitAmountDueToSupplier" />
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PriceQualifierCode" type="SRPriceQualifierCode" />
	<xs:element name="ProductIdentifier">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ProductIDType" />
				<xs:element ref="IDTypeName" minOccurs="0" />
				<xs:element ref="IDValue" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ProductIDType" type="SRProductIDType" />
	<xs:element name="PublisherIdentifier">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PublisherIDType" />
				<xs:element ref="IDTypeName" minOccurs="0" />
				<xs:element ref="IDValue" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PublisherIDType" type="SRPartyIDType" />
	<xs:element name="PublisherDetail">
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:element ref="PublisherIdentifier" maxOccurs="unbounded" />
					<xs:element ref="PublisherName" minOccurs="0" />
				</xs:sequence>
				<xs:element ref="PublisherName" />
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="PublisherName" type="xs:string" />
	<xs:element name="PublisherParty" type="SRParty" />
	<xs:element name="PurposeCode" type="SRPurposeCode" />
	<xs:element name="Quantity" type="xs:int" />
	<xs:element name="ReportingParty" type="SRParty" />
	<xs:element name="ReportPriceType" type="SRPriceQualifierCode" />
	<xs:element name="ReceivingParty" type="SRParty" />
	<xs:element name="ReferenceCoded">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ReferenceTypeCode" />
				<xs:element ref="ReferenceTypeName" minOccurs="0" />
				<xs:choice>
					<xs:sequence>
						<xs:element ref="ReferenceNumber" />
						<xs:choice minOccurs="0">
							<xs:element ref="ReferenceDate" />
							<xs:element ref="ReferenceDateTime" />
						</xs:choice>
					</xs:sequence>
					<xs:element ref="ReferenceDate" />
					<xs:element ref="ReferenceDateTime" />
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ReferenceDate" type="SRDateOrDateTime" />
	<xs:element name="ReferenceDateTime" type="SRDateTime" />
	<xs:element name="ReferenceNumber" type="xs:string" />
